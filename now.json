{
  "version": 2,
  "name": "create-issue-branch",
  "alias": "robvanderleek.now.sh",
  "builds": [
    { "src": "*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/now.js" }
  ],
  "env": {
    "APP_ID": "@app_id_secret",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@private_key_secret",
    "WEBHOOK_SECRET": "@webhook_secret_secret"
  }
}
